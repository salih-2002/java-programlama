import static java.lang.Math.PI;
import java.util.Scanner;
public class gradgradyanhesaplama {
    

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
      Scanner input=new Scanner (System.in);
      System.out.println("lütfen derece cinsinden açı giriniz");
      double derece=input.nextDouble();
      double grad= derece*(360/400);
      double radyan= derece*PI/180;
      System.out.println("radyana çevir;"+radyan);
      System.out.println("grada çevir;"+grad);
      
        
        
        
        
     
      
        
        
                  
        
        
      
       

        
                
                
        // TODO code application logic here
    }
    
}
