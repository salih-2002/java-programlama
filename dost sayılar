import java.util.Scanner;
public class dostsayılar {

   
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner input=new Scanner(System.in);
        System.out.println("bir tam sayı giriniz");
        int a=input.nextInt();
        System.out.println("bir tam sayı giriniz");
        int b=input.nextInt();
        int ta=0;
        int tb=0;
        System.out.println("a nın tam bölenleri= ");
        for(int i=1;i<a;i++){
            if(a%i==0){
                ta=ta+i;
                System.out.println(+i);
               
            } 
           
        }
         System.out.println("b nin tam bölenleri =");
        for(int i=1;i<b;i++){
            if(b%i==0){
                tb=tb+i;
                System.out.println(+i);
            }
        }
        if(ta==b && tb==a){
            System.out.println("bu sayılar dost sayılardır");
        }
        else{
            System.out.println("bu sayılar dost sayılar değildir");
        }
        
        
                
    }
    
}
