import java.util.Scanner;
public class abdoğruparçasınıaltınoranabölmek {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner input=new Scanner(System.in);
        System.out.println("ab doğru parçasının uzunluğu giriniz");
        double ab=input.nextDouble();
        double ac,cb;     //öyle bir c noktası bulunmalıdır ki aşağıdaki eşitlik sağlansın.
       cb=2*ab/(1+(5^1/2)); //cb/ac=ab/cb=1+5^1/2 sağlanmalıdır.
        ac=ab-cb;
        System.out.println(ac+" "+cb);
    }
    
}
